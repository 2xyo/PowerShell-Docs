{
    "build": {
        "content": [{
            "files": [
                "**/**.yml",
                "**/**.md"
            ],
            "exclude": [
                "**/obj/**",
                "**/includes/**",
                "_themes/**",
                "_themes.pdf/**",
                "README.md",
                "LICENSE",
                "LICENSE-CODE",
                "ThirdPartyNotices"
            ]
        }],
        "globalMetadata": {
            "breadcrumb_path": "/PowerShell/developer/breadcrumb/toc.json",
            "extendBreadcrumb": true,
            "ROBOTS": "INDEX, FOLLOW",
            "uhfHeaderId": "MSDocsHeader-Powershell",
            "ms.prod": "powershell",
            "ms.technology": "developer",
            "manager": "carmonm",
            "ms.devlang": "powershell",
            "ms.tgt_pltfr": "windows, macos, linux",
            "author": "joeyaiello",
            "ms.author": "jaiello"
        },
        "resource": [{
            "files": [
                "**/*.png",
                "**/*.jpg"
            ],
            "exclude": [
                "**/obj/**",
                "**/includes/**",
                "_themes/**",
                "_themes.pdf/**"
            ]
        }],
        "template": "op.html",
        "overwrite": [],
        "externalReference": [],
        "fileMetadata": {},
        "dest": "developer-docs",
        "markdownEngineName": "markdig"
    }
